{
  "presets": [
    ["@babel/preset-env"]
  ],

  "plugins": [
    "syntax-dynamic-import",
    "transform-object-rest-spread",
    ["transform-class-properties", { "spec": true }],
    ["prismjs", {
      "languages": ["javascript", "css", "markup", "go", "python", "ruby"],
      "plugins": ["line-numbers"],
      "css": true
    }]
  ]
}
